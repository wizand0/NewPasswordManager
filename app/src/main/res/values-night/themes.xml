<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.NewPasswordManager" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500_dark</item>
        <item name="colorPrimaryVariant">@color/purple_700_dark</item>
        <item name="colorOnPrimary">@color/white_dark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/purple_500_dark</item>
        <item name="colorSecondaryVariant">@color/purple_700_dark</item>
        <item name="colorOnSecondary">@color/white_dark</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="colorSurface">@color/colorSurface_dark</item>
        <item name="colorOnSurface">@color/colorOnSurface_dark</item>
        <item name="colorError">@color/colorError_dark</item>
        <item name="colorOnSurfaceVariant">@color/colorError_dark</item>
    </style>
</resources>